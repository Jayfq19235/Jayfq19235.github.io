{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
  "title": {
    "text": "New COVID-19 Cases over time in the UK",
    "subtitle":"",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },
  
   "data": {
    "url": "https://raw.githubusercontent.com/Jayfq19235/Jayfq19235.github.io/main/newCasesbyspecimendateLTLA.csv",
    "format": {"type": "csv"}
  },

   "transform":[
      
      {"filter": {
        "field": "areaName", 
        "oneOf": ["Brent", "Bristol, City of", "Portsmouth"]}
        }


    ],
  
  
  "height": 600,
  "width": 1000,
   
  "mark": {"type": "line", "point": false},
 
  "encoding": {

    "x": {
      "field": "date",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false}
    },

    "y": {
      "field":
      "newCasesBySpecimenDate",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    },

    "color": {
      "field": "areaName",
      "type": "nominal",
      "scale": {"scheme": "set1"},
      "title": "",
      "legend": {
        "orient": "top-left", 
        "fillColor": "#FcFdFd"}
    }
}
}
